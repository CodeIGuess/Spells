{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Spells",
	"patterns": [
		{
			"name": "keyword.control.spl",
			"begin": "^\\s*\\w+\\s*\\.\\s*$",
			"end": "(?=^\\s*(?!\\w+\\s*\\.\\s*))",
			"patterns": [
				{
					"name": "comment.block.spl",
					"match": "^\\s+.*$"
				}
			]
		},
		{
			"name": "constant.language.spl",
			"match": "^.*$"
		}
	],
	"repository": {},
	"scopeName": "source.spl"
}